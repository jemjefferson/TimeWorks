@model Employee

<h2 class="form-title">@Model.FirstName @Model.LastName</h2>
<form class="form-area" method="post">
    <div class="form-group">
        <table class="model-table">
            <tr>
                <th>Break Type</th>
            </tr>
            <tr>
                <td><button type="button" onclick="startBreak('y')" class="btn btn-primary btn-submit" role="button" >Paid</button></td>
            </tr>
            <tr>
                <td><button type="button" onclick="startBreak('n')" class="btn btn-primary btn-submit" role="button">Unpaid</button></td>
            </tr>
        </table>
    </div>
</form>
<div>
    <p class="form-buttons">
        <a asp-controller="" asp-action="" class="btn btn-primary btn-cancel">Cancel</a>
    </p>
</div>